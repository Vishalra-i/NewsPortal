{
    "version": 2,
    "builds": [
      {
        "src": "src/main.jsx",
        "use": "@vercel/node"
      },
      {
        "src": "public/index.html",
        "use": "@vercel/static"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  